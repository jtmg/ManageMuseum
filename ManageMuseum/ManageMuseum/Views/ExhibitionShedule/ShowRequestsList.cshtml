@model ManageMuseum.Models.Event

@{
    Layout = "~/Views/Shared/_SpaceManagerSharedLayout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ShowRequestsList</title>
</head>
<body>
<div class="container">
    
    
        <table class="table table-hover">
            <tr>
                <th> Name </th><th> Type </th><th> Start Date </th><th> Id Event </th>
            </tr>
        
            @foreach (var item in ViewBag.Data)
            {
                <tr>
                    <td>@item.Name</td><td>@item.EventType.Name</td><td>@item.StartDate</td><td>@item.Id</td>
                </tr>
            }       
        </table>
</div>
</body>
</html>
